<h1>New movie for <%= @list.name %></h1>

<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.input :comment %>
  <%= f.association :movie %>
  <p><%= f.submit "Submit movie", class: "btn btn-primary" %></p>
<% end %>

<div>
  <p><%= link_to "Back to list", lists_path, :class =>"btn btn-primary" %></p>
</div>
